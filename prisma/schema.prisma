generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id         Int      @id @default(autoincrement())
  name       String
  email      String   @unique
  password   String?
  active     Boolean  @default(true)
  created_at DateTime @default(now())
  updated_at DateTime @default(now())

  @@map("users")
}
